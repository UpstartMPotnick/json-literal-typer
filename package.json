{
  "name": "json-literal-typer",
  "version": "0.0.4",
  "description": "generates literal typescript iterfaces from JSON",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean:coverage": "rimraf coverage/*",
    "clean:build": "rimraf lib/*",
    "pretest": "npm rum clean:coverage",
    "test": "jest",
    "pretest:coverage": "npm rum clean:coverage",
    "test:coverage": "jest --coverage",
    "prebuild": "npm run clean:build",
    "build": "tsc",
    "lint": "eslint \"src/**/*.{js,ts,jsx,tsx}\"",
    "format": "prettier --write \"src/**/*.{ja,ts,jsx,tsx,json,html,css,scss,yml,yaml}\"",
    "format:check": "prettier --check \"src/**/*.{ja,ts,jsx,tsx,json,html,css,scss,yml,yaml}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest --config jestconfig.json",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --config jestconfig.json"
  },
  "keywords": [
    "json",
    "typescript",
    "literal"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pabra/json-literal-typer.git"
  },
  "contributors": [
    "Michael Scott-Nelson <mscottnelson@gmail.com> (https://github.com/mscottnelson)"
  ],
  "bugs": {
    "url": "https://github.com/pabra/json-literal-typer/issues"
  },
  "homepage": "https://github.com/pabra/json-literal-typer#readme",
  "author": "Patrick Braune <pabra@netzfruech.de>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^13.13.0",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-jest": "^22.19.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "peerDependencies": {},
  "dependencies": {}
}
